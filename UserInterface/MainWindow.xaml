<Window x:Class="UserInterface.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:pathfinderSheetViewModels="clr-namespace:PathfinderSheetViewModels;assembly=PathfinderSheetViewModels"
        mc:Ignorable="d"
        Title="Pathfinder Sheet v0.1" SizeToContent="WidthAndHeight" WindowStartupLocation="CenterScreen">

    <Window.DataContext>
        <pathfinderSheetViewModels:WindowViewModel />
    </Window.DataContext>

    <ContentControl Content="{Binding CurrentView}" />
</Window>
